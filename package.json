{
  "name": "be-inspired",
  "version": "1.0.0",
  "description": "Find your inspiration",
  "engines": {
    "node": "10.x"
  },
  "scripts": {
    "start": "node server",
    "build": "cd server/ && npm install && cd ../client/ && npm install && npm run build",
    "dev": "concurrently --kill-others \"cd server && npm start\" \"cd client && npm start\""
  },
  "cacheDirectories": [
    "server/node_modules",
    "client/node_modules"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/yoppoy/dare-to-inspire"
  },
  "keywords": [
    "node",
    "heroku",
    "create-react-app",
    "react"
  ],
  "license": "MIT",
  "devDependencies": {},
  "dependencies": {
    "concurrently": "^4.1.2"
  }
}
